<Application
    x:Class="Kebler.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:Kebler.UI.Converters;assembly=Kebler.UI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Kebler.UI;component/Themes/Generic.Dark.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
            <converters:ReverseBoolToVisibilityConverter x:Key="ReverseBoolToVisibilityConverter" />
            <converters:BoolToSlowImageConveter x:Key="BoolToSlowImageConveter" />
            <converters:GridWidthConverter x:Key="GridWidthConverter" />
            <converters:BytesToUserFriendlySpeed x:Key="BytesToUserFriendlySpeed" />
            <converters:BytesToUserFriendlyString x:Key="BytesToUserFriendlyString" />
            <converters:ObjectToVisibilityConverter x:Key="ObjectToVisibilityConverter" />
            <converters:ReverseBoolToBoolConverter x:Key="ReverseBoolToBoolConverter" />

        </ResourceDictionary>

    </Application.Resources>
</Application>